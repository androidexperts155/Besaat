                            android:src="@drawable/checked_circle" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rlLayoutmiddle"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rlLayouttop"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:background="@drawable/bg_blue_all_side_round_four_dp"
                        android:orientation="horizontal"
                        android:padding="@dimen/_20sdp">

                        <ImageView
                            android:id="@+id/ivQPay"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_alignParentStart="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/icon_pay" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginStart="@dimen/_14sdp"
                            android:layout_toEndOf="@+id/ivQPay"
                            android:fontFamily="@font/roboto_bold"
                            android:text="@string/q_pay"
                            android:textColor="@color/colorLightBlack"
                            android:textSize="@dimen/_15ssp" />

                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/unchecked_circle" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rlLayouttop"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textPayWith"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:background="@drawable/bg_blue_all_side_round_four_dp"
                        android:padding="@dimen/_20sdp">

                        <ImageView
                            android:id="@+id/ivVisa"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_30sdp"
                            android:layout_alignParentStart="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/icon_visa" />

                        <TextView
                            android:id="@+id/tvCreditCard"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toStartOf="@+id/ivCheckCredit"
                            android:layout_toEndOf="@id/ivVisa"
                            android:fontFamily="@font/roboto_bold"
                            android:paddingStart="@dimen/_14sdp"
                            android:paddingEnd="@dimen/_14sdp"
                            android:text="@string/credit_card"
                            android:textColor="@color/colorLightBlack"
                            android:textSize="@dimen/_15ssp" />

                        <ImageView
                            android:id="@+id/ivCheckCredit"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_alignParentEnd="true"
                            android:layout_centerVertical="true"
                            android:src="@drawable/unchecked_circle" />


                    </RelativeLayout>
                </RelativeLayout>


