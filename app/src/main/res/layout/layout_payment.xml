                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_20sdp"
                    android:layout_marginTop="@dimen/_14sdp"
                    android:layout_marginEnd="@dimen/_20sdp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageView122"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/iconpickup" />
                    <!--  android:background="@drawable/bg_light_whitish_blue_tl_br_bl_round_30"-->
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="18dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/pickUpAddress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="-2dp"
                            android:alpha=".3"
                            android:fontFamily="@font/roboto_regular"
                            android:includeFontPadding="false"
                            android:text="@string/pickup_address"
                            android:textAllCaps="true"
                            android:textColor="@color/colorLightBlack"
                            android:textSize="@dimen/_12ssp" />

                        <TextView
                            android:id="@+id/textView466"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1sdp"
                            android:fontFamily="@font/roboto_regular"
                            android:maxLines="2"
                            android:text="@string/address_pickup"
                            android:textColor="@color/colorLightBlack"
                            android:textSize="@dimen/_12ssp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
