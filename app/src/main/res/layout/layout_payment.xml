                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/icondropoff" />
                    <!--  android:background="@drawable/bg_light_whitish_blue_tl_br_bl_round_30"-->
                    <LinearLayout
