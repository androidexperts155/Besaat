<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:width="10dp"
        android:height="10dp"
        android:gravity="center"
        >
        <shape
            android:shape="oval">
            <solid
                android:color="@color/colorYellow"
                />
            <size
                android:width="@dimen/_20sdp"
                android:height="@dimen/_20sdp"
                />

            <stroke
                android:color="@color/colorYellow"
                android:width="1dp"/>
        </shape>
    </item>
    <item
        android:width="20dp"
        android:height="20dp"
        android:gravity="center">
        <shape
            android:shape="oval">
            <solid
                android:color="@android:color/transparent"
                />
            <size
                android:width="@dimen/_20sdp"
                android:height="@dimen/_20sdp"
                />
            <stroke
                android:color="@color/colorYellow"
                android:width="1dp"/>
        </shape>
    </item>
</layer-list>